@import url('https://fonts.googleapis.com/css?family=Dosis:700')
navcolor = #002f30
bgcolor = #d1ccbe
colorp = white //color letra principal 
bgsidebar = #006d55
btnclick = rgba(255, 170, 0, .2)
ficons = 26px
brand = 22px 'Dosis', sans-serif;
box-shadow()
	-webkit-box-shadow arguments
	-moz-box-shadow arguments
	box-shadow arguments
transform()
	-webkit-transform arguments
    -moz-transform arguments
    -o-transform arguments
border-radius()
	border-radius arguments
	-moz-border-radius arguments
	-webkit-border-radius arguments
	border arguments
transition()
	-webkit-transition arguments
  	-moz-transition arguments
  	-o-transition arguments
  	transition arguments
body
	background-color bgcolor
ul 
	display inline-flex 
	list-style none
nav ul li
	color colorp
	cursor pointer
	display inline-flex
	padding 10px
*
	margin 0
	padding 0
.navbar
	background-color navcolor
	box-shadow 2px 3px 10px -2px rgba(0,0,0,0.75)
	z-index 1
.btn
	font-size ficons
.btn:active, .social:active
	border-radius 50%
	transform scale(1.1)
	background-color btnclick
.brand
	vertical-align middle
	font brand
.brand:active
	transform scale(1.2)
.brand:hover, .btn:hover, .social:hover
	color bgcolor
.nav-right
	float right
	margin-top 10px
	margin-right 3px
.social
	background-color colorp
	border-radius 50%
	font-size 14px
	margin 3px
	padding 5px
.sidebar
	background-color bgsidebar
	box-shadow 6px -2px 12px -5px rgba(0,0,0,0.75)
	width 200px
	height 100%
	left 0px
	position absolute
	z-index -1
	